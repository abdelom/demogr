# Blueprint stairway_inference file
popid: stairway_inference # id of the population (no white space)
nseq: 20 # number of sequences
L: 2500.0 # total number of observed nucleic sites, including poly-/mono-morphic
whether_folded: True # folded (true) or unfolded (false) SFS
SFS: 70.5 25.0 20.5 17.5 19.5 8.5 18.0 12.0 5.0 10.5 # snp frequency spectrum: number of singleton, doubleton, etc.
