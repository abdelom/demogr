#largest_size_of_SFS_bin_used_for_estimation: 10
pct_training: 0.67 # percentage of sites for training
nrand: 5 9 14 18 # number of random break points for each try
project_dir: /home/damase/All/Cours/M2BI-Diderot/Species_evolution_inference/sei/inference/stairway_plot_v2.1.1/stairway_migration_m12=-4.0_kappa=-3.5_folded/stairway_inference # project directory
stairway_plot_dir: /home/damase/All/Cours/M2BI-Diderot/Species_evolution_inference/sei/inference/stairway_plot_v2.1.1/stairway_migration_m12=-4.0_kappa=-3.5_folded/stairway_plot_es
